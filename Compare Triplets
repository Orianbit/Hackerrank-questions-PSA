vector<int> compareTriplets(vector<int> a, vector<int> b) {
int count1=0;
    int count2=0;
    int n1=a.size();
    
    for(int i=0;i<n1;i++){
            if(a[i]>b[i]){
                count1++;
            }
            else if(a[i]<b[i]){
                count2++;
            }
        }
     return {count1,count2};
}
